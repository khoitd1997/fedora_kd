{
    "workbench.iconTheme": "Monokai Pro (Filter Spectrum) Icons",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "files.associations": {
        "*.cu": "c",
        "*.tdd": "makefile",
        "*.cfg": "tcl",
        "*.svd": "xml",
        "*.hpp": "cpp",
        "*.ks": "shellscript",
        "*.rasi": "css"
    },
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 0,
    "python.formatting.provider": "autopep8",
    "gitlens.keymap": "chorded",
    "editor.wordWrap": "bounded",
    "workbench.colorTheme": "Monokai Pro (Filter Spectrum)",
    "material-icon-theme.showUpdateMessage": false,
    "pasteImage.path": "${currentFileDir}/image",
    "editor.quickSuggestionsDelay": 2,
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": true,
    },
    "html.autoClosingTags": false,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "log.error",
                "settings": {
                    "foreground": "#af1f1f",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "log.warning",
                "settings": {
                    "foreground": "#f4ad42",
                    "fontStyle": ""
                }
            },
            {
                "scope": "log.info",
                "settings": {
                    "foreground": "#42f4e5",
                    "fontStyle": ""
                }
            }
        ]
    },
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "C_Cpp.loggingLevel": "Information",
    "git.enableSmartCommit": true,
    "editor.foldingStrategy": "indentation",
    "git.autofetch": true,
    "logFileHighlighter.customPatterns": [
        {
            "pattern": "\\[(.*?)\\]",
            "foreground": "#98f442"
        }
    ],
    "doxdocgen.generic.authorTag": "@author Khoi Trinh",
    "editor.detectIndentation": false,
    "go.formatTool": "gofmt",
    "editor.formatOnSave": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.formatOnPaste": true,
    "go.autocompleteUnimportedPackages": true,
    "window.titleBarStyle": "native",
    "editor.fontFamily": "source code pro",
    "editor.fontSize": "16",
    "todo-tree.defaultHighlight": {
        "type": "text",
        "foreground": "black",
        "background": "#6F6F70",
    },
    "vsicons.dontShowNewVersionMessage": true,
    "window.newWindowDimensions": "inherit",
    "window.restoreFullscreen": true,
    "workbench.activityBar.visible": false,
    "editor.hover.delay": 50,
    "editor.cursorBlinking": "phase",
    "search.showLineNumbers": true,
    "extensions.ignoreRecommendations": true,
    "window.menuBarVisibility": "hidden",
    "workbench.statusBar.feedback.visible": false,
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.statusBar.enabled": false,
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c99",
    "vim.useCtrlKeys": false,
    "multiCommand.commands": [
        {
            "command": "multiCommand.openFileAndCloseSideBar",
            "sequence": [
                "list.select"
            ]
        },
        {
            "command": "multiCommand.saveAndEnterCommand",
            "sequence": [
                "workbench.action.files.save",
                "extension.vim_escape"
            ]
        },
    ],
    "editor.hover.sticky": false,
    "terminal.integrated.fontFamily": "source code pro",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "keyboard.dispatch": "keyCode",
    "vim.cursorStylePerMode.normal": "block-outline",
    "vim.camelCaseMotion.enable": true,
    "vim.hlsearch": true,
    "files.autoSave": "afterDelay",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<Esc>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "leader",
                "leader",
                "W"
            ],
            "after": [
                "leader",
                "leader",
                "b"
            ]
        },
        {
            "before": [
                "leader",
                "leader",
                "E"
            ],
            "after": [
                "leader",
                "leader",
                "g",
                "e"
            ]
        }
    ],
    "editor.tabCompletion": "on",
    "path-autocomplete.triggerOutsideStrings": true,
    "editor.lineNumbers": "relative",
    "vim.sneak": true,
    "vim.iskeyword": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    // "vim.substituteGlobalFlag": true,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        // "<C-v>": true,
    },
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.smoothScrolling": true,
    "scrolloff.scrolloff": 25,
    "scrolloff.alwaysCenter": true,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.openEditorAtIndex1",
        "workbench.action.openEditorAtIndex2",
        "workbench.action.openEditorAtIndex3",
        "workbench.action.openEditorAtIndex4",
        "workbench.action.openEditorAtIndex5",
        "workbench.action.openEditorAtIndex6",
        "multiCommand.saveAndEnterCommand",
        "workbench.action.openRecent",
        "workbench.files.action.focusFilesExplorer",
        "workbench.view.extensions",
        "workbench.view.scm"
    ],
    "rewrap.autoWrap.enabled": true,
    "rewrap.wrappingColumn": 80,
    "editor.wordWrapColumn": 100,
    "vim.easymotion": true,
    "vim.easymotionMarkerFontFamily": "source code pro",
    "vim.easymotionMarkerWidthPerChar": 13,
    "vim.easymotionMarkerHeight": 16,
    "vim.easymotionMarkerForegroundColorTwoChar": "7bd88f",
    "vim.easymotionMarkerBackgroundColor": "orange",
    "workbench.editor.labelFormat": "short",
    "breadcrumbs.enabled": true
}