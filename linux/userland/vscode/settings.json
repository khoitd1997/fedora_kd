{
    "nix.serverPath": "rnix-lsp",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
    },
    "window.zoomLevel": 2,
    "python.formatting.provider": "autopep8",
    "gitlens.keymap": "chorded",
    "editor.minimap.enabled": false,
    "editor.wordWrap": "bounded",
    "workbench.colorTheme": "Monokai Pro",
    "pasteImage.path": "${currentFileDir}/image",
    "editor.quickSuggestionsDelay": 2,
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        },
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "html.autoClosingTags": false,
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "C_Cpp.loggingLevel": "Information",
    "git.autofetch": true,
    "doxdocgen.generic.authorTag": "@author Khoi Trinh",
    "editor.detectIndentation": false,
    "workbench.tips.enabled": false,
    "window.titleBarStyle": "custom",
    "window.title": "${rootPath}${separator}${activeEditorShort}",
    "editor.fontSize": 11,
    "window.newWindowDimensions": "inherit",
    "window.restoreFullscreen": true,
    "editor.hover.delay": 50,
    "editor.cursorBlinking": "solid",
    "search.showLineNumbers": true,
    "extensions.ignoreRecommendations": true,
    "extensions.autoUpdate": true,
    "gitlens.mode.statusBar.enabled": false,
    "gitlens.statusBar.enabled": false,
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c99",
    "terminal.integrated.scrollback": 5000,
    "keyboard.dispatch": "keyCode",
    "vim.cursorStylePerMode.normal": "block-outline",
    "vim.camelCaseMotion.enable": true,
    "vim.hlsearch": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        // {
        //   "before": ["<C-n>"],
        //   "commands": [":nohl"]
        // },
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "editor.action.addSelectionToNextFindMatch",
                "editor.action.startFindReplaceAction"
            ]
        },
        {
            "before": [
                "-"
            ],
            "commands": [
                "workbench.files.action.focusFilesExplorer"
            ]
        },
        // {
        //     "before": [
        //         "leader",
        //         "f"
        //     ],
        //     "commands": [
        //         "actions.find"
        //     ]
        // },
        // {
        //     "before": [
        //         "leader",
        //         "F"
        //     ],
        //     "commands": [
        //         "workbench.action.findInFiles"
        //     ]
        // },
        {
            "before": [
                "j"
            ],
            "after": [
                "j"
            ],
            "commands": [
                "editor.action.wordHighlight.trigger"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "k"
            ],
            "commands": [
                "editor.action.wordHighlight.trigger"
            ]
        },
        {
            "before": [
                "l"
            ],
            "after": [
                "l"
            ],
            "commands": [
                "editor.action.wordHighlight.trigger"
            ]
        },
        {
            "before": [
                "h"
            ],
            "after": [
                "h"
            ],
            "commands": [
                "editor.action.wordHighlight.trigger"
            ]
        },
        // {
        //     "before": [
        //         "k"
        //     ],
        //     "after": [
        //         "g",
        //         "k"
        //     ]
        // },
        {
            "before": [
                "leader",
                "leader",
                "W"
            ],
            "after": [
                "leader",
                "leader",
                "b"
            ]
        },
        {
            "before": [
                "leader",
                "o"
            ],
            "after": [
                "o",
                "<Esc>",
                "k",
                "O",
                "<Esc>",
                "j"
            ]
        },
        {
            "before": [
                "leader",
                "leader",
                "E"
            ],
            "after": [
                "leader",
                "leader",
                "g",
                "e"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        // {
        //   "before": ["<C-n>"],
        //   "commands": [":nohl"]
        // },
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "editor.action.addSelectionToNextFindMatch",
                "editor.action.startFindReplaceAction"
            ]
        }
    ],
    "editor.tabCompletion": "on",
    "path-autocomplete.triggerOutsideStrings": true,
    "editor.lineNumbers": "relative",
    "vim.sneak": true,
    "vim.iskeyword": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "vim.leader": "<space>",
    "vim.useCtrlKeys": false,
    "vim.handleKeys": {
        "<C-h>": true
    },
    "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.smoothScrolling": true,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.openEditorAtIndex1",
        "workbench.action.openEditorAtIndex2",
        "workbench.action.openEditorAtIndex3",
        "workbench.action.openEditorAtIndex4",
        "workbench.action.openEditorAtIndex5",
        "workbench.action.openEditorAtIndex6",
        "workbench.action.openRecent",
        "workbench.files.action.focusFilesExplorer",
        "workbench.action.togglePanelPosition",
        "workbench.view.extensions",
        "workbench.view.scm",
        "workbench.action.gotoSymbol",
        "workbench.action.gotoLine",
        "workbench.view.debug",
        "workbench.action.closeSidebar",
        "workbench.explorer.fileView.focus"
    ],
    "editor.wordWrapColumn": 100,
    "vim.easymotion": true,
    "vim.easymotionMarkerForegroundColorTwoCharFirst": "7bd88f",
    "vim.easymotionMarkerBackgroundColor": "orange",
    "workbench.editor.labelFormat": "short",
    "workbench.settings.openDefaultSettings": false,
    "breadcrumbs.enabled": true,
    "editor.bracketPairColorization.enabled": true,
    "workbench.editor.closeEmptyGroups": false,
    "editor.cursorStyle": "underline",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.errorSquiggles": "Enabled",
    "C_Cpp.default.includePath": [
        "~/.conan/data"
    ],
    "files.autoSaveDelay": 1,
    "workbench.tree.indent": 18,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.advanced.quickPick.closeOnFocusOut": false,
    "gitlens.blame.heatmap.location": "left",
    "gitlens.blame.highlight.enabled": false,
    "gitlens.codeLens.includeSingleLineSymbols": true,
    "gitlens.hovers.currentLine.changes": false,
    "gitlens.hovers.avatars": false,
    "gitlens.hovers.currentLine.details": false,
    "editor.foldingStrategy": "indentation",
    "bookmarks.showCommandsInContextMenu": false,
    "bookmarks.saveBookmarksInProject": false,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "vim.neovimPath": "",
    "explorer.autoReveal": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.largeFileOptimizations": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.tree.renderIndentGuides": "always",
    "editor.suggestSelection": "first",
    "remote.SSH.defaultExtensions": [
        "eamodio.gitlens",
        "sleistner.vscode-fileutils",
        "alefragnani.bookmarks",
        "compulim.vscode-clock",
        "mushan.vscode-paste-image",
        "k--kato.intellij-idea-keybindings",
        "dakara.dakara-foldplus",
        "visualstudioexptteam.vscodeintellicode",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "redhat.vscode-yaml",
        "mhutchie.git-graph",
        "donjayamanne.githistory",
        "yzhang.markdown-all-in-one",
        "shd101wyy.markdown-preview-enhanced",
        "ionutvmi.path-autocomplete",
        "ms-vscode.cpptools"
    ],
    "todo-tree.highlights.defaultHighlight": {
        "type": "text",
        "foreground": "#000000",
        "background": "#6F6F70"
    },
    "todo-tree.highlights.highlightDelay": 0,
    "todo-tree.tree.showCountsInTree": true,
    "todo-tree.general.tags": [
        "TODO",
        "FIXME",
        "HACK",
        "NOTE"
    ],
    "scm.defaultViewMode": "tree",
    "workbench.editor.limit.value": 10,
    "workbench.editor.limit.enabled": true,
    "terminal.integrated.cursorBlinking": true,
    "oneDarkPro.vivid": true,
    "cmake.preferredGenerators": [
        "Ninja",
        "Unix Makefiles"
    ],
    "cmake.generator": "Ninja",
    "editor.mouseWheelZoom": true,
    "workbench.startupEditor": "newUntitledFile",
    "python.languageServer": "Pylance",
    "redhat.telemetry.enabled": true,
    "yaml.schemas": {
        "file:///c%3A/Users/khoid/.vscode/extensions/atlassian.atlascode-2.10.9/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terminal.integrated.fontSize": 12,
    "remote.SSH.remotePlatform": {
        "kd-server": "linux"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "args": [
                "-noexit",
            ]
        }
    },
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x64)",
    "notebook.lineNumbers": "on",
    "notebook.consolidatedRunButton": true,
    "notebook.cellToolbarVisibility": "hover",
    "editor.guides.bracketPairs": true,
    "workbench.editor.wrapTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "editor.stickyTabStops": true,
    "editor.wordBasedSuggestionsMode": "allDocuments",
    "editor.formatOnSaveMode": "modifications",
    "workbench.colorCustomizations": { // Can customize each aspect
        "tab.activeBackground": "#7f8080" // Active Tab Top Highlighting
    },
    "files.associations": {
        "*.inc": "bitbake",
        "*.json": "jsonc"
    },
    "editor.suggest.showStatusBar": true,
    "atlascode.bitbucket.statusbar.showLogin": false,
    "atlascode.bitbucket.statusbar.showProduct": false,
    "atlascode.bitbucket.statusbar.showUser": false,
    "atlascode.jira.statusbar.showActiveIssue": false,
    "atlascode.jira.statusbar.showLogin": false,
    "atlascode.jira.statusbar.showProduct": false,
    "atlascode.jira.statusbar.showUser": false,
    "git.enableStatusBarSync": false,
    "git-graph.showStatusBarItem": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.statusBar.reduceFlicker": false,
    "workbench.iconTheme": "Monokai Pro Icons",
    "nix.enableLanguageServer": true
